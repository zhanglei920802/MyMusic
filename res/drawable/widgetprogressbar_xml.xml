<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ff333333" android:endColor="#ff333333" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <shape>
            <gradient android:startColor="#ff333333" android:endColor="#ff333333" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff10a54f" android:endColor="#ff10a54f" android:centerColor="#ff10a54f" />
            </shape>
        </clip>
    </item>
</layer-list>
